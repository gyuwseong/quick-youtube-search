{
  "name":"Quick Youtube Search",
  "version":"1.0.0",
  "description":"quick search for Youtube",
  "manifest_version":3,
  "permissions": ["contextMenus"],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "action":{
     "default_icon":"icon.png",
     "default_title":"Quick Youtube Search",
     "default_popup":"popup.html"
  }
}
